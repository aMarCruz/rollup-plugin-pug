{
  "name": "rollup-plugin-pug",
  "version": "0.1.4",
  "description": "Transforms Pug (aka Jade) templates to ES6 modules.",
  "main": "dist/rollup-plugin-pug.js",
  "module": "dist/rollup-plugin-pug.es.js",
  "jsnext:main": "dist/rollup-plugin-pug.es.js",
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "scripts": {
    "test": "mocha test/run",
    "pretest": "npm run build",
    "build": "rm -rf dist/* && node build-rt && rollup -c",
    "prepublish": "npm run lint && npm test",
    "lint": "eslint src/*.js test/run.js"
  },
  "keywords": [
    "rollup",
    "rollup-plugin",
    "javascript",
    "es6",
    "modules",
    "jade",
    "pug"
  ],
  "author": "aMarCruz <amarcruzbox-git@yahoo.com>",
  "license": "MIT",
  "homepage": "https://github.com/aMarCruz/rollup-plugin-pug",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aMarCruz/rollup-plugin-pug.git"
  },
  "bugs": {
    "url": "https://github.com/aMarCruz/rollup-plugin-pug/issues"
  },
  "dependencies": {
    "gen-pug-source-map": "^0.1.2",
    "pug": ">=2.0.0-rc.1 <3",
    "rollup-pluginutils": "^2.x"
  },
  "devDependencies": {
    "eslint": "^3.x",
    "mocha": "^3.2.0",
    "pug-runtime": "^2.x",
    "rollup": "^0.41.0",
    "rollup-plugin-buble": "^0.15.0"
  }
}
